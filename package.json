{
  "name": "jest-watch-options",
  "version": "1.1.0",
  "description": "A Jest watch mode plugin to change CLI options while Jest is running",
  "keywords": [
    "jest",
    "watch",
    "cli",
    "options"
  ],
  "main": "src/index.js",
  "author": {
    "name": "Tim Seckinger",
    "email": "seckinger.tim@gmail.com",
    "url": "https://github.com/jeysal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeysal/jest-watch-options"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest --watch",
    "test-once": "jest",
    "lint": "prettier -l \"**/*.{js,json,md}\"",
    "format": "prettier --write \"**/*.{js,json,md}\""
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "ansi-escapes": "^3.1.0",
    "jest-watcher": "^23.4.0",
    "yargs": "^12.0.5"
  }
}
